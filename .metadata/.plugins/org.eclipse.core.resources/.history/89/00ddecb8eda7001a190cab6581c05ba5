#include "main.h"

#ifdef MCU_TARGET_9

int main(void) {
	volatile unsigned long i;

	user_gpio_init();

	LPC_GPIO_PORT->DIR[0] &= ~(1UL << 0);
	LPC_GPIO_PORT->DIR[0] &= ~(1UL << 1);

	while (1) {
		LPC_GPIO_PORT->SET[0] = (1 << 4);
		for(i = 0; i < 100000; i++){ }
		LPC_GPIO_PORT->CLR[0] = (1 << 4);
		for(i = 0; i < 100000; i++){ }
	}

	return 0;
}

#endif
