#include "main.h"

#ifdef MCU_TARGET_9

int main(void) {
	volatile unsigned long i;

	user_delay_init();
	user_gpio_init();

	LPC_GPIO_PORT->DIR[0] &= ~(1UL << 0);
	LPC_GPIO_PORT->DIR[0] &= ~(1UL << 1);

	while (1) {
		//user_gpio_toggle();
		user_gpio_set();
		user_delay_ms(100);
		user_gpio_clear();
		user_delay_ms(100);
	}

	return 0;
}

#endif
